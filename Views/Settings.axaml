<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="TableTennis.Views.Settings">
    <StackPanel>
        <CheckBox x:Name="RubetsCheckBox" Content="Рубец-интеграция" />
        <CheckBox x:Name="DembelFactsCheckBox" Content="Включить дембель-факты" />
        <Slider x:Name="DembelFactsDelaySlider" Minimum="1" Maximum="30"  />
    </StackPanel>
</UserControl>
